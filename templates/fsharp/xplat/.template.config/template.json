{
  "$schema": "http://json.schemastore.org/template",
  "author": "AvaloniaUI",
  "classifications": [ "Desktop", "Xaml", "Avalonia", "Web", "Mobile" ],
  "description": "A cross-platform Avalonia UI Application using the MVVM pattern targetting Desktop and Browser with WebAssembly",
  "name": "Avalonia Cross Platform Application",
  "identity": "Avalonia.App.CrossPlatform.FSharp",
  "groupidentity": "Avalonia.App.CrossPlatform",
  "shortName": "avalonia.xplat",
  "defaultName": "AvaloniaApplication",
  "tags": {
    "language": "F#",
    "type": "project"
  },
  "sourceName": "AvaloniaTest",
  "preferNameDirectory": true,
  "symbols": {
    "TargetFrameworkOverride": {
      "type": "parameter",
      "description": "Overrides the target framework",
      "replaces": "TargetFrameworkOverride",
      "datatype": "string",
      "defaultValue": ""
    },
    "Framework": {
      "type": "parameter",
      "description": "The target framework for the project.",
      "datatype": "choice",
      "choices": [
        {
          "choice": "net7.0",
          "description": "Target net7.0"
        }
      ],
      "replaces": "FrameworkParameter",
      "defaultValue": "net7.0"
    }
  }
}